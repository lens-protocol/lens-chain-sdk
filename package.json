{
  "name": "@lens-network/sdk",
  "version": "0.1.0",
  "private": true,
  "description": "The quickest way to connect your dApp to the Lens Network.",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./viem": {
      "types": "./dist/viem.d.ts",
      "import": "./dist/viem.js"
    },
    "./ethers": {
      "types": "./dist/ethers.d.ts",
      "import": "./dist/ethers.js"
    }
  },
  "typesVersions": {
    "*": {
      "viem": [
        "./dist/viem.d.ts"
      ],
      "ethers": [
        "./dist/ethers.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "lint:code": "eslint . --ignore-pattern 'examples/*'",
    "lint:examples": "eslint examples/",
    "lint": "pnpm lint:code",
    "prepare:examples:ethers_eip-1193": "pnpm i -C examples/ethers_eip-1193",
    "prepare:examples:ethers_networks": "pnpm i -C examples/ethers_networks",
    "prepare:examples:viem_networks": "pnpm i -C examples/viem_networks",
    "prepare:examples": "pnpm prepare:examples:ethers_eip-1193 && pnpm prepare:examples:ethers_networks && pnpm prepare:examples:viem_networks",
    "prepublish": "pnpm clean && pnpm build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "lens-network",
    "sdk",
    "lens",
    "web3"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.1.2"
  },
  "packageManager": "pnpm@9.1.2",
  "devDependencies": {
    "@changesets/cli": "^2.27.3",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "ethers": "^6.12.1",
    "rimraf": "^5.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "viem": "^2.12.0",
    "zksync-ethers": "^6.7.1"
  },
  "peerDependencies": {
    "ethers": "^6.12.1",
    "viem": "^2.12.0",
    "zksync-ethers": "^6.7.1"
  },
  "peerDependenciesMeta": {
    "ethers": {
      "optional": true
    },
    "zksync-ethers": {
      "optional": true
    },
    "viem": {
      "optional": true
    }
  }
}
